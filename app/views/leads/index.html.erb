

  <div class="row">
    <div class="col s12 m12 l12">
     <div class="container ">


<p id="notice"><%= notice %></p>

<h3>Leads Dashboard</h3>
<%= link_to 'Add New Lead', new_lead_path , :class => "waves-effect waves-light btn"%>
<%= link_to 'Export To Excel', new_lead_path , :class => "waves-effect waves-light btn"%>


<table class="striped responsive-table">
  <thead>
    <tr class="highlight">
      <th>Name</th>
      <th>Phone</th>
      <th>String</th>
      <th>Company</th>
      <th>Email</th>
      <th>Origin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.name %></td>
        <td><%= lead.phone %></td>
        <td><%= lead.string %></td>
        <td><%= lead.company %></td>
        <td><%= lead.email %></td>
        <td><%= lead.origin %></td>
        <td><%= link_to 'Show', lead %></td>
        <td><%= link_to 'Edit', edit_lead_path(lead) %></td>
        <td><%= link_to 'Destroy', lead, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>

</div>

</div>
